{
  "name": "@seek/splunk-logger",
  "version": "0.2.7",
  "description": "Generates Splunk consumable logs in node 4.3.2 AWS Lambdas.",
  "main": "index.js",
  "private": false,
  "engines": {
    "node": "4.3.2"
  },
  "scripts": {
    "doc": "jsdoc2md logger.js > readme.md",
    "build": "babel logger.js --presets babili --no-comments --out-file index.js",
    "prepublish": "npm run build && npm run doc",
    "lint": "eslint ./",
    "fix": "eslint fix ./",
    "test": "mocha --opts mocha.opts",
    "test:watch": "mocha --opts mocha.opts --watch"
  },
  "homepage": "https://github.com/SEEK-Org/splunk-logger",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SEEK-Org/splunk-logger.git"
  },
  "contributors": [
    "Jack Jordan",
    "Jack Grigg"
  ],
  "license": "UNLICENSED",
  "devDependencies": {
      "babel-cli": "^6.18.0",
      "babel-preset-babili": "0.0.8",
      "chai": "^3.5.0",
      "chai-as-promised": "^6.0.0",
      "eslint": "^3.9.1",
      "eslint-config-airbnb-base": "^10.0.1",
      "eslint-plugin-import": "^2.2.0",
      "eslint-plugin-promise": "^3.3.1",
      "jsdoc": "^3.4.3",
      "jsdoc-to-markdown": "^2.0.1",
      "mocha": "^3.1.2",
      "proxyquire": "^1.7.10",
      "sinon": "^1.17.6",
      "sinon-chai": "^2.8.0"
  }
}
