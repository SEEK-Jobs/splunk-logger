{
  "name": "node-lambda-tools-logger",
  "version": "0.1.3",
  "description": "A tiny structured logging tool designed for Splunk.",
  "main": "index.js",
  "engines": {
    "node": "4.3.2"
  },
  "scripts": {
    "doc": "jsdoc2md logger.js > readme.md",
    "build": "babel logger.js --presets babili --no-comments --out-file index.js",
    "prepublish": "npm run build && npm run doc",
    "lint": "eslint ./",
    "fix": "eslint fix ./",
    "test": "mocha --opts mocha.opts",
    "test:tc": "mocha --opts mocha.opts --reporter mocha-teamcity-reporter",
    "test:watch": "mocha --opts mocha.opts --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SEEK-Jobs/node-lambda-tools.git"
  },
  "publishConfig": {
    "registry": "http://npmregistryinternal.seekinfra.com:4873"
  },
  "contributors": [
    "Jack Jordan",
    "Jack Grigg"
  ],
  "license": "ISC",
  "homepage": "https://github.com/SEEK-Jobs/node-lambda-tools/tree/master/logger",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-babili": "0.0.8",
    "jsdoc": "^3.4.3",
    "jsdoc-to-markdown": "^2.0.1"
  }
}
